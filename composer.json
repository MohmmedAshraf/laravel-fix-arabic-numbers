{
    "name": "outhebox/laravel-fix-arabic-numbers",
    "description": "Auto Convert all Arabic numbers input to English Numbers For Laravel framework & nova",
    "keywords": [
        "laravel",
        "middleware",
        "arabic",
        "numbers"
    ],
    "homepage": "https://github.com/MohmmedAshraf/laravel-fix-arabic-numbers",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Ashraf",
            "email": "cupo.ashraf@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.27",
        "squizlabs/php_codesniffer": "3.5.6",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "OutheBox\\FixArabicNumbers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OutheBox\\FixArabicNumbers\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "vendor/bin/phpcs --standard=psr2 src/",
        "fix-style": "vendor/bin/phpcbf --standard=psr2 src/",
        "clover": "vendor/bin/codacycoverage clover"
    },
    "config": {
        "sort-packages": true
    }
}
